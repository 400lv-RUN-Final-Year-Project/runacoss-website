{
  "name": "runacoss-server",
  "version": "1.0.0",
  "description": "RUNACOSS Website Server - Backend API for Redeemer's University Association of Computer Science Students",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "nodemon src/server.ts",
    "build": "tsc",
    "start": "node src/server.js",
    "test": "node src/helpers/testDb.js",
    "test:db": "node src/helpers/testDb.js",
    "init:db": "node src/helpers/initDb.js",
    "lint": "echo \"No linting configured\" && exit 0",
    "setup": "npm install && npm run init:db",
    "clean": "rm -rf node_modules package-lock.json",
    "reset": "npm run clean && npm install"
  },
  "keywords": [
    "runacoss",
    "university",
    "student-portal",
    "nodejs",
    "express",
    "mongodb",
    "api"
  ],
  "author": "RUNACOSS Team",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.10.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.0",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "express-xss-sanitizer": "^2.0.0",
    "googleapis": "^148.0.0",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.17.0",
    "mongoose": "^8.13.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "nodemailer": "^6.10.1",
    "node-fetch": "^2.6.7",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "qrcode": "^1.5.4",
    "speakeasy": "^2.0.0",
    "twilio": "^5.7.3",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/runacoss/runacoss-server.git"
  },
  "bugs": {
    "url": "https://github.com/runacoss/runacoss-server/issues"
  },
  "homepage": "https://github.com/runacoss/runacoss-server#readme"
